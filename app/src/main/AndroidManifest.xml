<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.team.comearnapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity"></activity>

        <service
            android:name="com.example.team.monitorlib.components.MonitorService"
            android:enabled="true"
            android:exported="false" />
        <service android:name="com.example.team.monitorlib.components.QueryAppsIntentService" />

        <activity
            android:name=".test.MockActivity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".test.SecondMockActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>